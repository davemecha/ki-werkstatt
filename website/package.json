{
  "name": "website",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "start": "node .output/server/index.mjs",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "lint": "eslint",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix",
    "preview:gh": "pnpm build && rm -rf .gh-preview && mkdir -p .gh-preview/ki-werkstatt && cp -r .output/public/* .gh-preview/ki-werkstatt/ && cp .output/public/index.html .gh-preview/ki-werkstatt/404.html && touch .gh-preview/ki-werkstatt/.nojekyll && http-server .gh-preview -p 4173 -c-1"
  },
  "dependencies": {
    "@fontsource/poppins": "^5.2.6",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-devtools": "^0.2.2",
    "@tanstack/react-router": "^1.130.2",
    "@tanstack/react-router-devtools": "^1.131.5",
    "@tanstack/react-router-ssr-query": "^1.131.7",
    "@tanstack/react-start": "^1.131.7",
    "@tanstack/router-plugin": "^1.121.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
    "reveal.js": "^5.2.1",
    "tailwindcss": "^4.0.6",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@tanstack/eslint-config": "^0.3.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/reveal.js": "^5.2.0",
    "@vitejs/plugin-react": "^4.3.4",
    "http-server": "^14.1.1",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.7.2",
    "vite": "^6.3.5",
    "vitest": "^3.0.5",
    "web-vitals": "^4.2.4"
  }
}
